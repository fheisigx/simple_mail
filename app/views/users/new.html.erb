<% provide(:title, "Sign In") %>

<h1>Sign Up</h1>

<div class="row">
  <div class="col-md-6 col-md-offset-3">

<%= form_for(:user, :url => {:action => 'create'}) do |f| %>
	  
	   <%= render(:partial => "form", :locals => {:f => f}) %>

	   	  <%= f.submit "Create my account", class: "btn btn-primary" %>

<% end %>

</div>
</div>